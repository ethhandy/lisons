{
  "name": "lisons",
  "version": "0.0.1",
  "description": "FIXME",
  "keywords": ["FIX", "ME"],
  "homepage": "https://fauu.github.io/lisons",
  "bugs": "https://github.com/fauu/lisons/issues",
  "license": "GPL-3.0",
  "author": "Piotr Grabowski <fau999@gmail.com> (https://github.com/fauu)",
  "repository": {
    "type": "git",
    "url": "https://github.com/fauu/lisons.git"
  },
  "main": "out/main.bundle.js",
  "scripts": {
    "clean-out": "rimraf out",
    "webpack:dev": "webpack --config webpack.dev.js",
    "webpack:prod": "webpack --config webpack.prod.js",
    "build:dev": "run-s clean-out webpack:dev",
    "build:prod": "run-s clean-out webpack:prod",
    "dev-server": "webpack-dev-server --config webpack.dev.js",
    "electron:dev": "cross-env NODE_ENV=dev electron .",
    "dev-server-and-electron": "run-p dev-server electron:dev",
    "dev": "run-s build:dev dev-server-and-electron",
    "electron:prod": "electron .",
    "prod": "run-s build:prod electron:prod",
    "dist:linux": "electron-builder --linux --x64",
    "dist:macos": "electron-builder --macos",
    "dist:windows": "electron-builder --windows --x64"
  },
  "build": {
    "appId": "io.github.fauu.lisons",
    "productName": "Lisons!",
    "copyright": "Copyright Â© 2017 Lisons authors",
    "compression": "store",
    "files": "out",
    "extraFiles": "LICENSE.md",
    "mac": {
      "category": "public.app-category.education"
    },
    "linux": {
      "description": "FIXME",
      "synopsis": "FIXME",
      "maintainer": "Piotr Grabowski <fau999@gmail.com>",
      "vendor": "Piotr Grabowski <fau999@gmail.com>",
      "target": ["AppImage"],
      "depends": [],
      "category": "Education",
      "packageCategory": "education"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@types/electron-debug": "^1.1.0",
    "@types/kuromoji": "^0.0.28",
    "@types/lodash": "^4.14.74",
    "@types/node": "^8.0.27",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-hot-loader": "^3.0.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-webpack-plugin": "^0.1.1",
    "cross-env": "^5.0.5",
    "electron": "^1.7.5",
    "electron-builder": "^19.27.3",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.3.4",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "@gxl/epub-parser": "fauu/epub-parser#dev",
    "@types/react-css-transition-replace": "^2.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chinese-tokenizer": "^0.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "devtron": "^1.4.0",
    "dexie": "^2.0.0-beta",
    "electron-devtools-installer": "^2.2.0",
    "electron-localshortcut": "^2.0.2",
    "file-loader": "^0.11.2",
    "franc-min": "^3.1.1",
    "happypack": "^4.0.0",
    "isbinaryfile": "^3.0.2",
    "kuromoji": "^0.1.1",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.minby": "^4.6.0",
    "lodash.zip": "^4.2.0",
    "mdi-react": "^2.0.46",
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "mobx-utils": "^3.1.1",
    "react": "^15.6.1",
    "react-css-transition-replace": "^2.2.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "styled-components": "^2.1.2",
    "tslib": "^1.7.1",
    "webpack-merge": "^4.1.0"
  },
  "babel": {
    "presets": ["es2015", "stage-0", "react"],
    "plugins": ["react-hot-loader/babel"]
  },
  "private": true
}
