{
  "name": "clairlire",
  "version": "1.0.0",
  "main": "out/main.bundle.js",
  "license": "MIT",
  "scripts": {
    "electron": "cross-env NODE_ENV=development npm-run electron .",
    "server": "npm-run webpack-dev-server",
    "build:main": "rimraf out && npm-run webpack",
    "build:production":
      "rimraf out && npm-run webpack --config webpack.config.production.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-hot-loader": "^3.0.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.5",
    "electron": "^1.7.5",
    "electron-builder": "^19.27.3",
    "html-webpack-plugin": "^2.30.1",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.3.4",
    "typescript": "^2.5.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-preset-minify": "^0.2.0",
    "babel-webpack-plugin": "^0.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "babel": {
    "presets": ["react", "es2015-node"],
    "plugins": ["react-hot-loader/babel"]
  }
}
